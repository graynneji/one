{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "typeRoots": [
      "./node_modules/@types",
      "./types"
    ],
    "skipLibCheck": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "react-native-webrtc.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
